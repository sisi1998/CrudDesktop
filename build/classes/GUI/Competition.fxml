<AnchorPane id="AnchorPane" onMousePressed="#click" prefHeight="181.0" prefWidth="181.0" style="-fx-background-color: #23395d;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.CompetitionController">
