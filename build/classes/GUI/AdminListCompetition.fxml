<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="550.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.AdminListCompetitionController">
    <stylesheets>
        <URL value="@adminaddcompetition.css" />
    </stylesheets>
   <children>
      <TableView fx:id="tableview" layoutX="201.0" layoutY="91.0" prefHeight="368.0" prefWidth="599.0">
         <columns>
            <TableColumn fx:id="nom" prefWidth="54.0" text="Nom" />
            <TableColumn fx:id="arene" prefWidth="82.0" text="Arene" />
            <TableColumn fx:id="date" prefWidth="127.0" text="Date" />
            <TableColumn fx:id="statut" prefWidth="41.0" text="Statut" />
            <TableColumn fx:id="image" prefWidth="75.0" text="Image" />
            <TableColumn fx:id="winner" prefWidth="55.0" text="Gagnant" />
         </columns>
      </TableView>
      <Button fx:id="addCompetition" layoutX="251.0" layoutY="46.0" mnemonicParsing="false" onAction="#addCompetitionM" styleClass="add-btn" text="Ajouter Une Competition" />
      <Button fx:id="delete" layoutX="200.0" layoutY="467.0" mnemonicParsing="false" onAction="#supprimerC" styleClass="add-btn" text="Delete" />
      <AnchorPane prefHeight="550.0" prefWidth="200.0" styleClass="right-bar" stylesheets="@adminaddcompetition.css">
         <children>
            <Button fx:id="compC" layoutX="29.0" layoutY="458.0" mnemonicParsing="false" onAction="#espaceCompetition" styleClass="search-box" stylesheets="@adminaddcompetition.css" text="Espace Competition" />
            <Button fx:id="PerfC" layoutX="28.0" layoutY="494.0" mnemonicParsing="false" onAction="#EspacePerformance" styleClass="search-box" text="Espace Performance" />
            <HBox layoutX="1.0" layoutY="31.0" prefHeight="100.0" prefWidth="200.0">
               <styleClass>
                  <String fx:value="transparent" />
                  <String fx:value="shadow" />
               </styleClass>
            </HBox>
            <Label layoutX="65.0" layoutY="37.0" text="Espace" textFill="#fcfcfc">
               <font>
                  <Font name="Corbel Light" size="25.0" />
               </font>
            </Label>
            <Label layoutX="29.0" layoutY="81.0" text="Competiton" textFill="WHITE">
               <font>
                  <Font name="Corbel Light" size="25.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
