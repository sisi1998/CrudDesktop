      </TableView>
      <Button fx:id="addCompetition" layoutX="519.0" layoutY="497.0" mnemonicParsing="false" onAction="#addCompetitionM" text="Ajouter Une Competition" />
      <AnchorPane prefHeight="550.0" prefWidth="185.0" styleClass="menubar">
         <children>
            <Button fx:id="PerfC" layoutX="31.0" layoutY="174.0" mnemonicParsing="false" onAction="#EspacePerformance" text="Espace Performance" />
            <Button fx:id="compC" layoutX="39.0" layoutY="209.0" mnemonicParsing="false" onAction="#espaceCompetition" styleClass="right-bar" stylesheets="@adminaddcompetition.css" text="Espace Competition" />
